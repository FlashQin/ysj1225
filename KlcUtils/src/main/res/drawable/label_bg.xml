<!-- 标签的背景 label_bg -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 标签选中时的背景 -->
    <item android:state_selected="true">
        <shape>
            <corners android:radius="12dp" />
            <solid android:color="#925EFF" />
        </shape>
    </item>
    <!-- 标签的正常背景 -->
    <item>
        <shape>
            <stroke android:width="0.5dp" android:color="@color/bg_f4f4f4" />
            <corners android:radius="12dp" />
        </shape>
    </item>
</selector>
